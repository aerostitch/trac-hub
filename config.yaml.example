trac:
  sqlite:
    file: db/trac.db

github:
  user: mavam
  pass: pass
  repo: mavam/test
 
labels:
  priority:
    - ['Low', '@low']
    - ['High', '@high']
  type:
    - ['Feature Request', 'feature request']
    - ['Task', 'task']
    - ['Fastpath', 'fastpath']
    - ['Problem', 'problem']
    - ['[Pp]atch', 'patch']
    - ['Test Case Missing', 'test missing']
  version:
    - ['1\.3', 'v1.3']
    - ['1\.4', 'v1.4']
    - ['1\.5.*', 'v1.5']
    - ['2\.0', 'v2.0']
    - ['2\.1', 'v2.1']
    - ['git/master', 'git/master']
    - ['git/topic', 'git/topic']
  resolution:
    - ['Rejected', 'rejected']
    - ['Invalid', 'invalid']
    - ['Duplicate', 'duplicate']
    - ['Works for Me', 'works for me']

users:
  - [matthias, mavam]
  - [vallenti, mavam]
